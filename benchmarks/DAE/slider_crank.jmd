```julia
"""
ModelingToolkit symbolic form for automatic differentiation.
"""

using ModelingToolkit

@variables t
@variables p1(t) p2(t) x3(t) q1(t) q2(t) q3(t) q4(t)
@variables v1(t) v2(t) vx3(t) vq1(t) vq2(t) vq3(t) vq4(t)

Dp1 = Differential(t)(p1)
Dp2 = Differential(t)(p2)
Dx3 = Differential(t)(x3)
Dq1 = Differential(t)(q1)
Dq2 = Differential(t)(q2)
Dq3 = Differential(t)(q3)
Dq4 = Differential(t)(q4)

Dv1 = Differential(t)(v1)
Dv2 = Differential(t)(v2)
Dvx3 = Differential(t)(vx3)
Dvq1 = Differential(t)(vq1)
Dvq2 = Differential(t)(vq2)
Dvq3 = Differential(t)(vq3)
Dvq4 = Differential(t)(vq4)

function build_slider_crank_mtk()
    """Build symbolic system"""
    eqs = [
        Dp1 ~ v1,
        Dp2 ~ v2,
        Dx3 ~ vx3,
        Dq1 ~ vq1,
        Dq2 ~ vq2,
        Dq3 ~ vq3,
        Dq4 ~ vq4,
    ]
    
    # Constraints
    push!(eqs, 0 ~ p1 - OMEGA * t)
    push!(eqs, 0 ~ x3 - (L1 * cos(p1) + L2 * cos(p2) + q4 * cos(p2)))
    push!(eqs, 0 ~ 0 - (L1 * sin(p1) + L2 * sin(p2) + q4 * sin(p2)))
    push!(eqs, 0 ~ v1 - OMEGA)
    
    push!(eqs, Dv1 ~ 0)
    push!(eqs, Dv2 ~ 0)
    push!(eqs, Dvx3 ~ 0)
    push!(eqs, Dvq1 ~ 0)
    push!(eqs, Dvq2 ~ 0)
    push!(eqs, Dvq3 ~ 0)
    push!(eqs, Dvq4 ~ 0)
    
    return eqs
end

